    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/cuid": "^1.3.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.21",
